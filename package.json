{
  "name": "scantopaybackend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "set DEBUG=web-app && set NODE_ENV=development&& nodemon server.js --exec babel-node",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "drop:db": "set NODE_ENV=development&& sequelize db:drop",
    "undo:db": "sequelize db:migrate:undo:all",
    "server": "node ./dist-server/server.cjs",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel server.js --out-dir dist-server",
    "build": "babel src -d dist",
    "clean": "rm -rf dist/*",
    "start": "npm run clean && npm run build && node ./dist/server.js",
    "render": "yarn install && yarn run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/register": "^7.21.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.6.0"
  }
}
